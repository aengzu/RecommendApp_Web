<IfModule mod_url.c>
    checkURL On
    ServerEncoding EUC-KR
    ClientEncoding UTF-8
</IfModule>

<ifmodule mod_rewrite.c>
RewriteEngine On
RewriteBase /

RewriteCond %{HTTP_HOST} ^(www.pttoapp.com)$ [NC]
RewriteRule ^(.*)$ (www.pttoapp.com/$1) [R=301,L]

#RewriteCond %{HTTP_HOST} ^www\.hwaiiteammbti\.kr [NC]
#RewriteRule ^(.*)$ http://abc.kr/$1 [R=301,L]
# www 서브도메인을 이용하지 않음

RewriteCond %{HTTP_HOST} ^(testbmti\.pttoapp\.com) [NC]
RewriteCond $1 !^(testbmti)/
RewriteRule ^(.*)$ /testbmti/$1 [L]
# testMBTI 서브도메인은 폴더 디렉토리 연결

RewriteCond %{HTTP_HOST} ^favorite-worldcup-master\.pttoapp\.com [NC]
RewriteCond $1 !^(favorite-worldcup-master)/
RewriteRule ^(.*)$ /favorite-worldcup-master/$1 [L]
# testpidol 서브도메인은 폴더 디렉토리 연결

</ifmodule>